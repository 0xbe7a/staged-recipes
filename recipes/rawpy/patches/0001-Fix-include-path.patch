From 4eb725871ef108ac82ad1cd1c59f789b4e61c2b5 Mon Sep 17 00:00:00 2001
From: Jeongseok Lee <jeongseok@meta.com>
Date: Tue, 1 Apr 2025 08:51:37 -0700
Subject: [PATCH] Fix include path

---
 rawpy/data_helper.h | 4 ++--
 rawpy/def_helper.h  | 6 +++---
 2 files changed, 5 insertions(+), 5 deletions(-)

diff --git a/rawpy/data_helper.h b/rawpy/data_helper.h
index 5111546..6efb2d6 100644
--- a/rawpy/data_helper.h
+++ b/rawpy/data_helper.h
@@ -1,5 +1,5 @@
-#include "libraw.h"
-#include "libraw_types.h"
+#include "libraw/libraw.h"
+#include "libraw/libraw_types.h"
 
 // subset of libraw_colordata_t
 typedef struct
diff --git a/rawpy/def_helper.h b/rawpy/def_helper.h
index 3affb10..1565e78 100644
--- a/rawpy/def_helper.h
+++ b/rawpy/def_helper.h
@@ -1,5 +1,5 @@
-#include "libraw_version.h"
-#include "libraw_const.h"
+#include "libraw/libraw_version.h"
+#include "libraw/libraw_const.h"
 
 // xtrans supported since 0.16.0
 #ifndef LIBRAW_XTRANS
@@ -11,7 +11,7 @@
 // libraw_config.h only exists since 0.15.4, and only in cmake builds
 // hence all values defined below are irrelevant if < 0.15.4 or cmake wasn't used
 #if _HAS_LIBRAW_CONFIG_H
-#include "libraw_config.h"
+#include "libraw/libraw_config.h"
 #define _LIBRAW_HAS_FLAGS 1
 #else
 #define _LIBRAW_HAS_FLAGS 0
